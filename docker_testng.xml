<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TataSky Web Automation" verbose="1">
    <test name="Firefox Test" preserve-order="true" parallel="methods" thread-count="4">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="testcases.TestProfileManagement"></class>
            <class name="testcases.TestHomeScreen"></class>
        </classes>
    </test>
    <test name="Chrome Test" preserve-order="true">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="testcases.TestProfileManagement"></class>
            <class name="testcases.TestHomeScreen"></class>
        </classes>
    </test>
</suite>

